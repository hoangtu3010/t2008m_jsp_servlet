<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>GetListStudentServlet</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.students.GetListStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetListStudentServlet</servlet-name>
        <url-pattern>/students/list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateStudentServlet</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.students.CreateStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateStudentServlet</servlet-name>
        <url-pattern>/students/create</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteStudentServlet</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.students.DeleteStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteStudentServlet</servlet-name>
        <url-pattern>/students/delete</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EditStudentServlet</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.students.EditStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditStudentServlet</servlet-name>
        <url-pattern>/students/edit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetListProductServlet</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.products.GetListProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetListProductServlet</servlet-name>
        <url-pattern>/products/list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateProductServlet</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.products.CreateProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateProductServlet</servlet-name>
        <url-pattern>/products/create</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EditProductServlet</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.products.EditProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditProductServlet</servlet-name>
        <url-pattern>/products/edit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ShowShoppingCart</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.shoppingcart.ShowShoppingCart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowShoppingCart</servlet-name>
        <url-pattern>/carts/list</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddProductToCart</servlet-name>
        <servlet-class>com.example.hellot2008m.controller.shoppingcart.AddProductToCart</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddProductToCart</servlet-name>
        <url-pattern>/carts/add</url-pattern>
    </servlet-mapping>
</web-app>